?(( Rutina para el cálculo del conjunto potencia              ))
?(( Realizado por: Rodolfo González González.                 ))

((power1 () y z))
((power1 x () ()))
((power1 x1 (y1|y2) z)
    (union ((x1)) () z1)
    (union (x1) (y1) z2)
    (union z1 (z2) z3)
    (power1 x1 y2 z4)
    (union z3 z4 z))

((power2 () x y ()))
((power2 x y () ()))
((power2 (x1|x2) y (y1|y2) z)
    (union ((x1)) (y2) z1)
    (power1 x1 y z2)
    (union z1 z2 z3)
    (power2 x2 y2 y2 z4)
    (union z3 z4 z))

((power4 x () ()))
((power4 x (y1|y2) z)
    (union (x) y1 z1)
    (power4 x y2 z2)
    (union (z1) z2 z))

((power3 () y1 y2 ()))
((power3 (x1|x2) y1 y2 z)
    (power4 x1 y1 z1)
    (union z1 y2 z2)
    (power3 x2 y1 y2 z3)
    (union z1 z2 z))

((cp x z)
    (power2 x x x y1)
    (union (()) y1 y2)
    (union (x) y2 z1)
    (power3 x z1 z1 z))